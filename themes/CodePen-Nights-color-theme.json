{
  "name": "CodePen-Nights",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#1e1f27",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#88afbf"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string.constant.other.placeholder"],
      "settings": {
        "foreground": "#47cf73"
      }
    },
    {
      "name": "Meta JS",
      "scope": ["meta.import.js", "variable.other.readwrite.alias.js"],
      "settings": {
        "foreground": "#5e91f2"
      }
    },
    {
      "name": "Variables JS",
      "scope": ["variable.other.property.js", "variable.other.object.js"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "PHP Constants",
      "scope": ["constant.other.php"],
      "settings": {
        "foreground": "#ffdd40"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff1134"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#ae63e4"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9ca0b1"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["Keyword", "Storage"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation.definition.tag",
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#d5d7de"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation.definition.string.begin.ruby",
        "punctuation.definition.string.end.ruby"
      ],
      "settings": {
        "foreground": "#9ca0b1"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "support.type.object.module.js"
      ],
      "settings": {
        "foreground": "#ffdd40"
      }
    },
    {
      "name": "Tag JS",
      "scope": ["entity.other.attribute-name.js"],
      "settings": {
        "foreground": "#0ebeff"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#5e91f2"
      }
    },
    {
      "name": "C-related Block Level Variables",
      "scope": ["source.cpp meta.block variable.other"],
      "settings": {
        "foreground": "#ffcfcb"
      }
    },
    {
      "name": "Variables constant",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#ffcfcb"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#ffcfcb"
      }
    },
    {
      "name": "HTML Class",
      "scope": ["keyword.control.ruby"],
      "settings": {
        "foreground": "#ffdd40"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other",
        "string.quoted.single.js"
      ],
      "settings": {
        "foreground": "#2bc7b9"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "variable.parameter.function.language.special",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#ffdd40"
      }
    },
    {
      "name": "Symbols, Inherited Class, Markup Heading",
      "scope": [
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#0ebeff"
      }
    },
    {
      "name": "String, String Tempalte",
      "scope": ["string", "string.template.js"],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#2bc7b9"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#ffdd40"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#49dcff"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#5e91f2"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#ff1134"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff8d41"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5e91f2"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor",
        "meta.objectliteral.js"
      ],
      "settings": {
        "foreground": "#5e91f2"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ae63e4"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#ffdd40"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#5e91f2"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#47cf73"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff1134"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ae63e4"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#0ebeff"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#0ebeff"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5e91f2"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff1134"
      }
